<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<link rel="icon" type="image/png" href="{{ asset('build/images/favicon.png') }}"/>
		<title>
			{% block title %}Le mauvais coin{% endblock %}
		</title>
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body class="mt-5">
		{% block navbar %}
			{% include '_navbar.html.twig' %}
		{% endblock %}
		{% block flash %}{% include '_flash.html.twig' %}{% endblock %}
		{% block body %}{% endblock %}
		{% block footer %}{% endblock %}
	</body>
</html>
